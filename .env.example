# ========================================
# AstrBot Live2D 桌面端 - 环境配置示例
# ========================================
# 复制此文件为 .env.local 并根据实际情况修改

# WebSocket 连接配置
VITE_WS_URL=ws://localhost:9090/astrbot/live2d
VITE_WS_TOKEN=

# 调试模式
VITE_DEBUG_MODE=false
VITE_ENABLE_CONSOLE_LOG=true

# Live2D 模型配置
VITE_DEFAULT_MODEL_PATH=/models/default/model3.json
VITE_MODEL_SCALE=1.0

# 窗口配置（仅 Electron）
VITE_WINDOW_WIDTH=400
VITE_WINDOW_HEIGHT=600
VITE_ALWAYS_ON_TOP=false
VITE_TRANSPARENT=true

# 鼠标穿透配置
VITE_MOUSE_PASSTHROUGH_ENABLED=true
VITE_ALPHA_THRESHOLD=10

# 性能配置
VITE_ENABLE_HMR=true
VITE_OPTIMIZE_DEPS=true
